
<div class="row">
    <% products.forEach((product)=>{%>
     <div class= " col-sm-6 col-md-4">
        
            <div class="thumbnail">
        <img src="<%=product.image %>" class="img-responsive" style="max-height: 180px ;" >
        <div class="caption">
            <h3><%=product.Product_name%></h3>
            <p>T<%=product.Description %> </p>
            <span class="badge text-bg-warning pull-left"> <%=product.Cost %></span>
             <a href="/add-to-cart/<%=product._id %>"> <span class="badge text-bg-info pull-right">Add to Cart</span></a>
            
        </div>
    </div>
</div> 

<%  }) %>	
</div>


